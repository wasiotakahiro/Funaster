<h3>以下の内容で、送信する。</h3>

<%= form_with(model: @funastar, url: funastars_path, local: true) do |form| %>
  <%= form.hidden_field :image %>
  <%= form.hidden_field :caption %>
  <%= image_tag(@funastar.image) %>
  <%= form.hidden_field :image_cache %>
  <p>タイトル:<%= @funastar.image %></p>
  <p>本文:<%= @funastar.caption %></p>
  <%= form.submit  "登録する" %>
<% end %>

<%= form_with(model: @hunastar, url: new_funastar_path, local: true, method: 'get') do |form| %>
  <%= form.hidden_field :image %>
  <%= form.hidden_field :caption %>
  <%= form.submit "もどる" %>
<% end %>
